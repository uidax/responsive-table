{
  "compilerOptions": {
    "strictNullChecks": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "target": "es5",
    "lib": ["dom", "es2020"],
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}